{
  "expo": {
    "name": "guess_number_app",
    "slug": "guess_number_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/2048px-Instagram_icon.png",
    "splash": {
      "image": "./assets/laUmp.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rajkeshwani.guess_number_app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": ["ACCESS_FINE_LOCATION"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
